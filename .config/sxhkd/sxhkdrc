super + shift + q
	bspc quit

super + shift + r
	bspc wm -r

alt + F4 
	bspc node -c

super + Return
	alacritty

super + e
	pcmanfm

super + Print
	flameshot gui

super + r
	sh ~/.config/rofi/launcher/launcher.sh

super + { alt + }{Right,Down,Up,Left}
	bspc node -z {right 5, down 5, up 5, left 5}

super + { shift + }{t, space, f}
	bspc node -t {tiled, floating, fullscreen}

super + {_,shift + }{Right,Down,Up,Left}
	bspc node -{f,s} {east,south,north,west}

super + {_,shift + }{1-9,0}
	bspc {desktop -f,node --to-desktop} {1-9,10}




